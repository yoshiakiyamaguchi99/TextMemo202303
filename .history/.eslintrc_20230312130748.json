{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "airbnb"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "react/function-component-definition": [
      2,
      { "namedComponents": "function-declaration" }
    ],
    "no-use-before-define": 0,
    "quotes": [2, "single"]
  }
}
